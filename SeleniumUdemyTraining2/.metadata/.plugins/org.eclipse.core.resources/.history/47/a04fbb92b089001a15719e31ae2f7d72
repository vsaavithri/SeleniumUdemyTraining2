package checkConstructors;

public class Sample {
	
	
	static String name1 = "Sairam";
	 static int emp_id = 1000;
	  
	  
	  public static void display(String name1, int emp_id)
		{
			
			System.out.println("The name is "+name1);
			System.out.println("The emp id is " + emp_id);
		}

	public static void main(String[] args) {
		
		
		
		Sample s = new Sample();
		
		
		Sample.display("Sairam", 12);
		
	
		// TODO Auto-generated method stub
		
		
		
		 
		 
		
		
			 
			 
			 System.out.println("Name is "+ name1);
		 }
		
		
		
		


	}

}
