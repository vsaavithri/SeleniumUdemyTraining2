package ST2020;

public class CheckForIfElse {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a =1; 
		int b = 5;
		
		if (a<b)
		{
		
			System.out.println("A is less than B:");
			
		}
		
		else
			
		{
			System.out.println("A is NOT less than B:");
		}
			
			
		

	}

}

	