package checkConstructors;

public class Student {
	
	String name;
	int role_num;
	
	
	public  Student() {
		System.out.println("This is the Student 1");
	}
	
	public Student(String name, int role_num){
		
		this.name = name;
		this.role_num = role_num;
		
		
	}
		
		
	

	public static void main(String[] args) {
		
		Student s2 = new Student();
		
		Student s1 = new Student("Rahul", 100);
		s1.
		System.out.println(s1.name);
	    System.out.println(s1.role_num);

	}

}
